.dots {
    position: absolute;
    left: 0;
    right: 0;
    bottom: 20rpx;
    display: flex;
    width: 238rpx;
    margin: auto;
    border-radius: 10rpx;
    height: 20rpx;
    justify-content: center;
}

.dot {
    margin: 4rpx 8rpx;
    width: 30rpx;
    height: 6rpx;
    background: rgb(255,255,255);
    border: rgb(218,69,70) solid 1rpx;
    border-radius: 8rpx;
    transition: all 0.6s;
}

.active {
    width: 30rpx;
    background: #f80;
}

.yhq {
    position: relative;
    width: 100%;
    box-sizing: border-box;
    height: 200rpx;
    margin-bottom: 30rpx;
}

.yhqbgimg {
    position: absolute;
    width: 100%;
    height: 200rpx;
}

.yhqview {
    position: absolute;
    width: 100%;
    height: 200rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
}

.yhqleft {
    width: 79%;
    height: 200rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
}

.yhqleft1 {
    min-width: 150rpx;
    width: 30%;
    height: 150rpx;
    text-align: center;
    margin: 0 20rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 45rpx;
    color: #EE3452;
    font-weight: bolder;
}

.yhqleft1 view:nth-child(2) {
    font-size: 30rpx;
}

.yhqleft1 image {
    width: 140rpx;
    height: 140rpx;
    border-radius: 10rpx;
}

.yhqleft2 {
    height: 180rpx;
