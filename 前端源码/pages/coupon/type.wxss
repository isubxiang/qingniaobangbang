    justify-content: space-around;
}

.yhqleft2 view {
    width: 100%;
}

.yhqright {
    width: 22%;
    min-width: 21%;
    height: 160rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    align-items: center;
}

.yhqleft22 {
    font-size: 26rpx;
    color: #F15A73;
}

.yhqleft23 {
    font-size: 24rpx;
