@import "../../../wuBaseWxss/1.wxss";

.header {
    background: #06c1ae;
    text-align: center;
    padding: 0 40rpx 60rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.header .status {
    font-size: 32rpx;
    color: #fff;
}

.header .desc {
    margin: 30rpx 0 20rpx 0;
    color: #b1e4d1;
    font-size: 26rpx;
}

.header .desc view {
    margin-top: 10rpx;
}

.header .desc .time {
    margin: 0 8rpx;
    color: #fff;
}

.header form {
    width: 500rpx;
}

.header .buttons {
    display: flex;
    align-items: center;
    padding-top: 30rpx;
    width: 500rpx;
}

.header .buttons button {
    width: 180rpx;
    height: 64rpx;
    line-height: 64rpx;
    font-size: 28rpx;
    background: #06c1ae;
    color: #edf8f4;
    border-radius: 0;
    border: 1rpx solid #edf8f4;
}

.header .buttons button.submit {
    background: #fadb79;
    color: #ba6a2e;
    border: 0;
}

.header .cause {
    padding: 12rpx 40rpx;
    margin: 20rpx auto 0 auto;
    display: flex;
    background: #4fc299;
    border-radius: 10rpx;
    color: #228d68;
    font-size: 24rpx;
    align-items: center;
}

.header .cause .iconfont {
    font-size: 48rpx;
}

.header .cause .cause-info {
    display: flex;
    flex-direction: column;
    flex: 1;
    padding-left: 20rpx;
    text-align: left;
}

.header .cause .cause-info view {
    font-size: 28rpx;
    margin-bottom: 6rpx;
}

.body .user {
    display: flex;
    background: #fff;
    padding: 10rpx 40rpx;
    align-items: center;
}

.body .user .info avatar {
    width: 70rpx;
    height: 70rpx;
    background: #ddd;
    border-radius: 50%;
}

.body .user .info {
    display: flex;
    flex: 1;
}

.body .user .info .name {
    font-size: 30rpx;
    margin-left: 20rpx;
    line-height: 70rpx;
}

.body .user button {
    border-radius: 28rpx;
    color: #fff;
    line-height: 56rpx;
    height: 56rpx;
    width: 56rpx;
    padding: 0;
    background: #82d4b6;
    overflow: hidden;
    margin-left: 10rpx;
}

.body .user button text.iconfont {
    font-size: 42rpx;
    vertical-align: middle;
}

.body .user-hr {
    margin: 0;
}

.body .content {
    background: #fff;
    padding-bottom: 20rpx;
}

.body .content .remark {
    line-height: 1.5;
    margin: 0 40rpx;
    padding-top: 40rpx;
}

.body .content .text {
    font-size: 32rpx;
}

.body .content .remark .iconfont {
    color: #ddd;
    position: relative;
    margin: 0 8rpx;
    top: -10rpx;
}

.body .content .remark .secret {
    position: static;
    background: #06c1ae;
    color: #fff;
    border-radius: 50%;
    width: 48rpx;
    height: 48rpx;
    display: inline-block;
    text-align: center;
}

.body .shop {
    background: #fff;
    display: flex;
    padding: 20rpx 30rpx 0 30rpx;
}

.body .shop image {
    width: 50rpx;
    height: 50rpx;
    background: #ddd;
}

.body .shop .name {
    margin-left: 18rpx;
    line-height: 50rpx;
    font-size: 26rpx;
    color: #888;
}

.body .menu .line {
    height: 2rpx;
    background: #efefef;
}

.body .menu .item {
    padding: 20rpx 30rpx;
    display: flex;
}

.body .menu .item image {
    width: 110rpx;
    height: 110rpx;
    background: #ddd;
    margin-right: 18rpx;
    border: 1px solid #ddd;
}

.body .menu .item view {
    flex: 1;
}

.body .menu .item .name {
    display: block;
    margin: 12rpx 0 10rpx;
}

.body .menu .item .quantity {
    color: #888;
}

.body .menu .item .money {
    margin-top: 36rpx;
    font-size: 32rpx;
}

.body .attend {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin: 20rpx 40rpx 0 40rpx;
    background: #f7f7f7;
    padding: 10rpx;
    border-radius: 10rpx;
}

.body .attend .serial {
    width: 120rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    text-align: center;
    position: relative;
    top: -6rpx;
}

.body .attend .serial .code {
    font-size: 30rpx;
    color: #222;
}

.body .attend .serial .code text {
    font-size: 45rpx;
    margin-left: 8rpx;
}

.body .attend .serial .advance {
    font-size: 24rpx;
    color: #aaa;
}

.body .attend .item {
    display: flex;
    flex-direction: column;
    flex: 1;
    text-align: center;
}

.body .attend .item-padding {
    padding: 0 10rpx;
}

.body .attend .item .value {
    color: #888;
    font-size: 28rpx;
}

.body .attend .item .iconfont {
    margin-right: 6rpx;
    font-size: 26rpx;
}

.body .attend .item .caption {
    color: #ccc;
    font-size: 24rpx;
}

.body .attend .split {
    border-left: 1rpx solid #eee;
    width: 0;
    height: 60rpx;
}

.body .path {
    background: #fff;
    margin-top: 20rpx;
}

.body .path .item {
    padding: 30rpx 0;
    display: flex;
}

.body .path .left {
    margin: 0 20rpx 0 40rpx;
    width: 60rpx;
    text-align: center;
    color: #ccc;
    font-size: 24rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    padding-top: 6rpx;
}

.body .path .left .iconfont {
    font-size: 36rpx;
}

.body .path .item-start .info {
    height: 64rpx;
}

.body .path .item-start .left {
    padding-top: 18rpx;
    text-align: center;
}

.body .path .item-start .left .iconfont {
    font-size: 32rpx;
}

.body .path .info {
    height: 90rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    font-size: 30rpx;
}

.body .path .info .concat {
    color: #666;
    margin-top: 8rpx;
    font-size: 28rpx;
}

.body .path .info .concat text {
    margin-left: 10rpx;
}

.body .prop {
    background: #fff;
    margin: 20rpx 0;
}

.popup-track .list {
    background: #fff;
    height: 640rpx;
}

.popup-track .item {
    display: flex;
    height: 90rpx;
    line-height: 90rpx;
}

.popup-track .item .time {
    width: 300rpx;
    padding-left: 30rpx;
    color: #888;
    font-size: 26rpx;
}

.popup-track .item .content {
    flex: 1;
    color: #444;
}


.popup-track .pic-list image {
   padding: 10rpx;
}
.popup-track .pic-list .DelivererNickName {
   display: flex;
    flex-direction: column;
    color: #ff0000;
    padding: 20rpx;
}


.dialog-profile {
    background: #fff;
    border-radius: 30rpx;
    width: 600rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.dialog-profile .avatar {
    position: relative;
    height: 80rpx;
}

.dialog-profile .nickname {
    font-size: 32rpx;
    color: #06c1ae;
}

.dialog-profile .desc {
    margin-top: 20rpx;
    text-align: center;
    font-size: 30rpx;
}

.dialog-profile avatar {
    width: 160rpx;
    height: 160rpx;
    border-radius: 50%;
    position: absolute;
    top: -80rpx;
    left: -80rpx;
    z-index: 2;
    background: #fff;
}

.dialog-profile button {
    margin: 40rpx;
    background: #06c1ae;
    border-radius: 20rpx;
    color: #fff;
    line-height: 90rpx;
    display: block;
    width: 400rpx;
}

.dialog-profile button .iconfont {
    font-size: 50rpx;
    vertical-align: middle;
    margin-right: 10rpx;
}

.dialog-finish {
    background: #fff;
    border-radius: 30rpx;
    width: 530rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    padding: 40rpx;
}

.dialog-finish image {
    width: 350rpx;
    height: 253rpx;
    margin: 40rpx 0;
}

.dialog-finish .desc {
    color: #666;
    font-size: 30rpx;
    padding: 30rpx 0;
}

.dialog-finish .notice {
    color: #ba6a2e;
    margin-top: 20rpx;
    display: block;
}

.dialog-finish .notice .strong {
    font-weight: bold;
}

.dialog-finish button {
    width: 100%;
    margin-top: 20rpx;
    border-radius: 10rpx;
}

.popup-tip,.popup-appraise {
    padding: 40rpx 30rpx;
    display: flex;
    flex-direction: column;
}

.popup-tip .items {
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
}

.popup-tip .item {
    width: 30%;
    margin-bottom: 30rpx;
    border: 1rpx solid #ddd;
    height: 78rpx;
    line-height: 76rpx;
    border-radius: 6rpx;
    text-align: center;
    font-size: 32rpx;
}

.popup-tip .item.actived {
    border: 1rpx solid #ff9800;
    color: #ff9800;
    display: flex;
    justify-content: center;
}

.popup-tip .item-other {
    flex: 1;
    width: auto;
    margin-bottom: 0;
    display: block;
}

.popup-tip .item-other.actived .symbol {
    display: inline;
}

.popup-tip .item-other.actived input {
    width: 100rpx;
    height: 76rpx;
    border: 0;
    text-align: left;
    margin-left: 10rpx;
}

.popup-appraise textarea {
    height: 160rpx;
    width: auto;
    display: block;
}

.popup-appraise .labels {
    border-top: 1rpx solid #e0e0e0;
}

.popup-appraise .labels {
    display: flex;
    flex-wrap: wrap;
    padding-top: 12rpx;
    margin-top: 18rpx;
}

.popup-appraise .labels .item {
    white-space: nowrap;
    border-radius: 8rpx;
    padding: 2rpx 10rpx;
    margin-top: 18rpx;
    margin-right: 18rpx;
    font-size: 26rpx;
    border: 1rpx solid #999;
    color: #999;
}

.popup-appraise .labels .merit {
    border-color: #06c1ae;
    color: #06c1ae;
}

.popup-appraise .labels .fault {
    border-color: #ff9800;
    color: #ff9800;
}

.popup-appraise .labels .disabled {
    opacity: 0.3;
}

.popup-appraise .labels .checked {
    color: #fff;
}

.popup-appraise .labels .merit.checked {
    background: #06c1ae;
}

.popup-appraise .labels .fault.checked {
    background: #ff9800;
}

.popup-appraise .labels .common.checked {
    background: #999;
}

.popup-appraise .labels .iconfont {
    font-size: 24rpx;
    margin-left: 6rpx;
}

.popup-giveup {
    background: #fff;
}

.popup-giveup .radio-group {
    display: flex;
    flex-direction: column;
}

.popup-giveup .radio-group .radio {
    font-size: 32rpx;
    height: 90rpx;
    line-height: 90rpx;
    padding-left: 40rpx;
}

.popup-giveup .radio-group .radio-other {
    display: flex;
    padding-right: 20rpx;
}

.popup-giveup .radio-group .radio-other input {
    flex: 1;
    height: 90rpx;
    line-height: 90rpx;
    margin-left: 8rpx;
}

.modal-content {
    font-size: 30rpx;
}

.modal-notice {
    color: #ba6a2e;
    margin-top: 10rpx;
    font-size: 30rpx;
}

button[plain] {
  color:#353535;
  border:1px solid #fff;
  background-color:transparent;
}
