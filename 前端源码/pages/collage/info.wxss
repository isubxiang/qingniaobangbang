@import "../../wuBaseWxss/12.wxss";

page {
    width: 100%;
    height: 100%;
}

.container {
    overflow: hidden;
}

.head {
    width: 100%;
    height: 650rpx;
}

.head image {
    width: 100%;
    height: 100%;
}

.head swiper {
    width: 100%;
    height: 100%;
}

.slide-image {
    width: 100%;
    height: 100%;
}

.Collage {
    width: 100%;
    background: linear-gradient(to right,rgb(249,75,2),rgb(252,190,79));
    bottom: 0;
    padding: 10rpx 20rpx;
}

.Collage image {
    width: 40rpx;
    height: 40rpx;
}

.collMan {
    border-radius: 6rpx;
    padding: 0 10rpx;
    border: 2rpx solid rgb(255,228,183);
}

.Collage>view:nth-child(3) {
    margin-right: 40rpx;
}

.coTime>view:nth-child(2) {
    margin-top: 10rpx;
}

.coTime>view:nth-child(1) {
    color: rgb(200,79,16);
}

.coTime>view text {
    margin-right: 5rpx;
}

.coTime>view text:nth-child(odd) {
    background: #fff;
    color: #000;
    padding: 5rpx 7rpx;
    border-radius: 8rpx;
}

.shop>view:nth-child(1) {
    width: 85%;
}

.shop>view:nth-child(2) {
    width: 13%;
    border-left: 2rpx solid #ccc;
}

.shop>view image {
    width: 40rpx;
    height: 40rpx;
    display: block;
    margin: 10rpx auto 10rpx;
}

.buyShop {
    position: fixed;
    bottom: 0;
    width: 100%;
    height: 110rpx;
}

.buyShop image {
    width: 50rpx;
    height: 50rpx;
    display: block;
    margin: 10rpx auto 10rpx;
}

.buyShop>view {
    height: 100%;
}

.buyShop>view:nth-child(1) {
    width: 20%;
}

.buyShop>view:nth-child(2) {
    width: 30%;
    background: rgb(248,156,7);
}

.buyShop>view:nth-child(3) {
    width: 50%;
    background: rgb(240,5,5);
}

.collInfo {
}

.collInfoNum image {
    width: 30rpx;
    height: 30rpx;
}

.collInfoNum>view {
    width: 50%;
}

.group image {
    width: 90rpx;
    height: 90rpx;
    border-radius: 50%;
}

.group>view:nth-child(1) {
    width: 40%;
}

.group>view:nth-child(2) {
    width: 35%;
}

.group>view:nth-child(3) {
    width: 20%;
    margin-left: 5%;
}

.parGroup text {
    padding: 10rpx 20rpx;
    background: linear-gradient(to right,rgb(248,84,0),rgb(255,192,107));
    border-radius: 10rpx;
}

.group:last-child {
    border: none;
}

.rule {
}

.rule>view:nth-child(1) {
    width: 20%;
}

.rule>view:nth-child(2) {
    width: 15%;
}

.rule>view:nth-child(3) {
    width: 20%;
}

.rule>view:nth-child(4) {
    width: 15%;
}

.rule>view:nth-child(5) {
    width: 30%;
}

.shopInfo {
    width: 100%;
}

.shopInfo>image {
    width: 100%;
}

.share {
    position: fixed;
    bottom: 0;
    width: 100%;
    height: 100%;
    z-index: 5000000000;
}

.share>view:nth-child(1) {
    background: #000;
    opacity: 0.5;
    width: 100%;
    height: 100%;
}

.share image {
    width: 130rpx;
    height: 130rpx;
}

.share>view:nth-child(2) {
    bottom: 0;
    height: 330rpx;
    width: 100%;
    background: #f9f9f9;
}

.share_info>view {
    width: 50%;
    height: 250rpx;
}

.share_info>view view:nth-child(1) {
    margin: 20rpx auto 20rpx;
    background: #fff;
    height: 140rpx;
    border-radius: 50%;
    width: 140rpx;
}

.share_info>view button {
    width: 100%;
    margin: 0;
    padding: 0;
    top: 0;
    z-index: 5000000000000;
    height: 100%;
    background: none;
    border-radius: 0;
}

.generateImage {
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    z-index: 50000000000;
}

.generateImage>view view:nth-child(1) {
    width: 80%;
    height: 70%;
    border: 2rpx solid #ccc;
    margin: 12% auto;
    border-radius: 15rpx;
}

.generateImage>view view:nth-child(4) {
    top: 10rpx;
    right: 10rpx;
    color: #fff;
    width: 60rpx;
    height: 60rpx;
    font-size: 50rpx;
    line-height: 60rpx;
    border-radius: 50%;
    background: #000;
}

.generateImage>view:nth-child(1) {
    width: 100%;
    height: 100%;
    background: #000;
    opacity: 0.5;
    top: 0;
    z-index: -100;
}

.generateImage>view:nth-child(2) {
    width: 90%;
    height: 90%;
    background: #fff;
    border-radius: 15rpx;
}

.generateImage view button {
    width: 60%;
    background: orange;
    color: #fff;
    height: 80rpx;
    line-height: 80rpx;
    margin-top: -30rpx;
    border-radius: 60rpx;
}

.generateImage view image {
    width: 100%;
    height: 100%;
}

.se_right {
    width: 35rpx;
    height: 35rpx;
}

.address>image:nth-child(3) {
    right: 20rpx;
}

.address>view:nth-child(2) {
    width: 80%;
    margin-left: 5%;
}

.yhxq {
    padding: 0rpx 30rpx 20rpx;
    font-size: 26rpx;
    color: #999;
    text-align: start;
}

.yhxqimg {
    width: 100%;
    margin-top: 20rpx;
}

.yhxqimg image {
    width: 100%;
}