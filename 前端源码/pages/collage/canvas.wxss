.imagePathBox {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 10;
}

.shengcheng {
    width: 80%;
    height: 80%;
    position: fixed;
    top: 50rpx;
    left: 10%;
    border-radius: 15rpx;
    z-index: 10;
}

.baocun {
    display: block;
    width: 80%;
    height: 80rpx;
    padding: 0;
    line-height: 80rpx;
    text-align: center;
    position: fixed;
    bottom: 50rpx;
    left: 10%;
    background: #fff;
    color: #333;
    font-size: 32rpx;
    border-radius: 44rpx;
}

button[class="baocun"]::after {
    border: 0;
}