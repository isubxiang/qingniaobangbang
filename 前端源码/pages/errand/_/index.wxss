}

.bottomView image {
    width: 500rpx;
    height: 64rpx;
    display: block;
    margin: 0 auto;
}

.home-fixed .inputButton {
    width: 98%;
    line-height: 56rpx;
    text-align: center;
    font-size: 24rpx;
    color: #fff;
    margin: 0;
}


.nav {
    color: #fff;
}

.nav-c {
    background: #fff;
    color: black;
}

.nav view,.nav-c view {
    display: inline-block;
}

.home-communtiy {
    max-width: 100%;
    height: 46rpx;
    line-height: 46rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    color:#fff;

}

.home-position {
    background-color:rgba(0,0,0,0.20);
    border-radius:10rpx;
    padding:0rpx 10rpx;

}

.home-inputxt {
    float: right;
    width: 58%;
    height: 58rpx;
    background: rgba(200,200,200,0.5);
    border-radius: 30rpx;
    margin-right: 4%;
    position: relative;
}

.home-downicon {
    vertical-align: top;
    margin-top: 2rpx;
    margin-left: 16rpx;
}

.home-inputxt input {
    padding-left: 45rpx;
}







.body {
    background: #fff;
}

.body-top .caption {
    padding: 20rpx 20rpx 10rpx 20rpx;
