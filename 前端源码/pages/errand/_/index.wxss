}

.body-list .item .content {
    font-size: 32rpx;
    line-height: 44rpx;
}

.body-list .item .content .secret {
    position: static;
    background: #06c1ae;
    color: #fff;
    border-radius: 50%;
    line-height: 40rpx;
    width: 40rpx;
    height: 40rpx;
    display: inline-block;
    text-align: center;
    font-size: 28rpx;
    margin-right: 4rpx;
}

.body-list .item .type {
    color: #fff;
    background: #ddd;
    font-size: 24rpx;
    padding: 2rpx 10rpx;
    border-radius: 4rpx;
    margin-right: 10rpx;
}

.body-list .item .desc {
    font-size: 26rpx;
    color: #aaa;
    margin-top: 16rpx;
    line-height: 40rpx;
}

.body-list .item .time {
    font-size: 22rpx;
    color: #ccc;
    margin-right: 20rpx;
}

.body-list .item .right {
    padding-right: 30rpx;
}

.body-list .item .right view {
    font-size: 24rpx;
    color: #666;
    text-align: center;
}

.body-list .item .right .money {
    color: #ff9800;
    font-size: 30rpx;
}

.body-list .item .right button {
    background: #06c1ae;
    border-radius: 10rpx;
    height: 64rpx;
    line-height: 64rpx;
    width: 160rpx;
    font-size: 32rpx;
    margin-top: 10rpx;
    padding: 0;
}

.body-list .item .right button.disabled {
    background: #d0d0d0;
    color: #f7f7f7;
}

.body-list .item .right button.accepted {
    border: none;
    background: #f0f0f0;
    color: #888;
}

.body-list .item .right button.finished {
    border: none;
    background: none;
    color: #888;
}

.empty {
