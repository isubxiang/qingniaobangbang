.title {
    font-size: 30rpx;
}

.title .iconfont {
    font-size: 40rpx;
    vertical-align: middle;
}

.header {
    position: relative;
}

.header-bg {
    background: #06c1ae;
    padding-top: 10rpx;
    z-index: 1;
    width: 100%;
}

.header-content {
    z-index: 2;
    left: 0;
    width: 100%;
}

.header-top {
    padding-left: 20rpx;
    display: flex;
    justify-content: flex-start;
}

.header .title {
    color: #fff;
}

.header .nearest {
    border-radius: 24rpx;
    font-size: 24rpx;
    height: 48rpx;
    line-height: 46rpx;
    background: #fff;
    color: #666;
    opacity: 0.6;
    padding: 0 18rpx;
    margin-left: 20rpx;
    margin-right: 30rpx;
}

.header .nearest .iconfont {
    position: relative;
    top: 2rpx;
    margin-right: 4rpx;
}

.header .input {
    margin: 20rpx 0rpx 24rpx 0rpx;
    background: #fff;
    border-radius: 0rpx;
    min-height: 100rpx;
    margin-top: 20rpx;
    overflow: hidden;
    display: flex;
    flex-direction: column;
    z-index: 2;
}

.header .input .form {
    margin: 12rpx 30rpx 0 24rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 72rpx;
}

.header .input .form .dot {
    width: 16rpx;
    height: 16rpx;
    border-radius: 8rpx;
    display: inline-block;
    background: #ff9800;
}

.header .input .form .txt {
    font-size: 30rpx;
    padding-left: 20rpx;
    flex: 1;
    margin-right: 60rpx;
    height: 100%;
    line-height: 80rpx;
    color: #575757;
    box-sizing: border-box;
    display: inline-block;
}

.header .input .form .btn {
    background: #06c1ae;
    color: #fff;
    font-size: 32rpx;
    border-radius: 8rpx;
    height: 72rpx;
    line-height: 72rpx;
    padding: 0 30rpx;
    display: inline-block;
    margin-top: 10rpx;
}

.header .input swiper {
    flex: 1;
    margin-bottom: 12rpx;
}

.header .input swiper swiper-item {
    display: flex;
    width: 100%;
    flex-wrap: wrap;
    padding-bottom: 40rpx;
    box-sizing: border-box;
}

.header .input swiper .item {
    background: #f0f0f0;
    color: #aaa;
    font-size: 24rpx;
    text-align: center;
    width: 23%;
    display: flex;
    flex-direction: column;
    margin: 12rpx 1% 4rpx 1%;
    height: 100rpx;
    justify-content: center;
    position: relative;
    padding: 5rpx 0;
}

.header .input swiper .finished1 {
    background:#fff;
    color: #aaa;
    font-size: 24rpx;
    text-align: center;
    width: 18%;
    display: flex;
    flex-direction: column;
    margin: 5rpx 1% 5rpx 1%;
    height: 120rpx;
    justify-content: center;
    position: relative;
    padding: 5rpx 0;
}

.header .input swiper .finished1 image {
    width: 75rpx;
    height: 75rpx;
    background: #fff;
}

.header .input swiper .finished1 .Keyword{
    width: 100%;
    height: 45rpx;
    line-height: 45rpx;
    font-size: 28rpx;
    color: #666;
}

.udlr {
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
}



.header .input swiper .item .hot {
    position: absolute;
    right: 8rpx;
    top: -8rpx;
    color: #fff;
    background: #de534b;
    font-size: 14rpx;
    padding: 2rpx 6rpx;
}

.header .input swiper .item .tag {
    position: absolute;
    right: -6rpx;
    top: -14rpx;
    color: #fff;
    background: #06c1ae;
    font-size: 18rpx;
    padding: 0 6rpx;
}

.header .input swiper .item .key {
    margin: 0 auto 6rpx auto;
    font-size: 30rpx;
    color: #444;
    width: 100rpx;
}

.header .input .swiper-bar {
    position: absolute;
    width: 100%;
    left: 0;
    bottom: 12rpx;
    display: flex;
    justify-content: center;
}

.header .input .swiper-bar view {
    width: 30rpx;
    height: 6rpx;
    background: #ddd;
    margin: 0 2rpx;
}

.header .input .swiper-bar view.actived {
    background: #06c1ae;
}

.banner {
    height:175px;
    width: 100%;
    display: black;
    z-index: 1;
}

.banner .bg,.banner .bg image {
    width: 100%;
    height: 390rpx;
}

.banner swiper {
    height: 180rpx;
    width: 100%;
    margin-bottom: 10rpx;
}

.banner swiper swiper-item {
    position: relative;
}

.banner swiper navigator {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
}

.banner swiper image {
    height: 100%;
    width: 100%;
}



.home-fixed {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 60rpx;
    padding: 35rpx 0 20rpx 3%;
